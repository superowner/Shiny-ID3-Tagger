{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "https://github.com/ShinyId3Tagger/Shiny-ID3-Tagger/settings.default.json",
	"type": "object",
	"properties": {
		"LogLevel": {
			"type": "integer",
			"minimum": 0,
			"maximum": 4
		},
		"PreferTags": {
			"type": "boolean"
		},
		"RemoveVersion": {
			"type": "boolean"
		},
		"RemoveFeaturing": {
			"type": "boolean"
		},
		"OverwriteImage": {
			"type": "boolean"
		},
		"AutoCollectFiles": {
			"type": "boolean"
		},
		"AutoSearch": {
			"type": "boolean"
		},
		"AutoUpdate": {
			"type": "boolean"
		},
		"AutoCapitalize": {
			"type": "boolean"
		},
		"ThresholdRedValue": {
			"type": "integer",
			"minimum": 0,
			"maximum": 100
		},
		"MaxImageSize": {
			"type": "integer",
			"minimum": 1,
			"maximum": 9999
		},
		"UserAgent": {
			"type": "string"
		},
		"FilenamePatterns": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"CoverPriority": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"type": "string",
				"enum": [
					"Napster (Rhapsody)",
					"Discogs",
					"Qobuz",
					"Tidal",
					"Genius",
					"7digital",
					"Deezer",
					"iTunes",
					"Last.fm",
					"Spotify",
					"Gracenote (Sony)",
					"Amazon",
					"Musicbrainz",
					"Netease",
					"QQ (Tencent)"
				]
			}
		},
		"LyricsPriority": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"type": "string",
				"enum": [
					"Apiseeds",
					"Viewlyrics",
					"Lololyrics",
					"Chartlyrics",
					"Netease",
					"Xiami"
				]
			}
		}
	},
	"additionalProperties": false,
	"required": [
		"LogLevel",
		"PreferTags",
		"RemoveVersion",
		"RemoveFeaturing",
		"OverwriteImage",
		"AutoCollectFiles",
		"AutoSearch",
		"AutoUpdate",
		"AutoCapitalize",
		"ThresholdRedValue",
		"MaxImageSize",
		"UserAgent",
		"FilenamePatterns",
		"CoverPriority",
		"LyricsPriority"
	]
}